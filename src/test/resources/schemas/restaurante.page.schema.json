{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "required": [
    "content",
    "pageable",
    "last",
    "totalPages",
    "totalElements",
    "first",
    "size",
    "number",
    "sort",
    "numberOfElements",
    "empty"
  ],
  "properties": {
    "content": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "id",
          "nome",
          "localizacao",
          "horarioFuncionamento",
          "capacidade"
        ],
        "properties": {
          "id": {
            "type": "string"
          },
          "nome": {
            "type": "string"
          },
          "localizacao": {
            "type": "string"
          },
          "horarioFuncionamento": {
            "type": "string"
          },
          "capacidade": {
            "type": "number"
          }
        }
      }
    },
    "pageable": {
      "type": "string"
    },
    "last": {
      "type": "boolean"
    },
    "totalPages": {
      "type": "integer"
    },
    "totalElements": {
      "type": "integer"
    },
    "first": {
      "type": "boolean"
    },
    "size": {
      "type": "integer"
    },
    "number": {
      "type": "integer"
    },
    "sort": {
      "type": "object",
      "required": [
        "empty",
        "sorted",
        "unsorted"
      ],
      "properties": {
        "empty": {
          "type": "boolean"
        },
        "sorted": {
          "type": "boolean"
        },
        "unsorted": {
          "type": "boolean"
        }
      }
    },
    "numberOfElements": {
      "type": "integer"
    },
    "empty": {
      "type": "boolean"
    }
  }
}